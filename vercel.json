{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/api-lib/index?path=$1" }
  ],
  "redirects": [
    { "source": "/admin", "destination": "/admin/dashboard", "permanent": false }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
}